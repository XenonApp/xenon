{
    "modes": {
        "javascript": {
            "commands": {
                "Tools:Check": {
                    "fn": "check",
                    "options": {
                        "undef": true,
                        "unused": true,
                        "esversion": 6,
                        "devel": true,
                        "browser": true,
                        "node": true,
                        "laxcomma": true,
                        "laxbreak": true,
                        "lastsemic": true,
                        "onevar": false,
                        "passfail": false,
                        "maxerr": 1000,
                        "expr": true,
                        "multistr": true,
                        "globalstrict": true
                    },
                    "inputs": {
                        "text": true
                    },
                    "internal": true,
                    "sandbox": "check"
                },
                "Tools:Index": {
                    "fn": "index",
                    "inputs": {
                        "text": true
                    },
                    "internal": true,
                    "sandbox": "index"
                },
                "Tools:Beautify": {
                    "fn": "beautify",
                    "inputs": {
                        "text": true,
                        "preferences": true
                    }
                }
            },
            "handlers": {
                "index": [
                    "Tools:Index"
                ],
                "check": [
                    "Tools:Check"
                ]
            }
        }
    }
}
