        "clojure": {
            "commands": {
                "Tools:Brackets:Check": {
                    "fn": "check",
                    "internal": true,
                    "inputs": {
                        "text": true
                    },
                    "brackets": ["()", "{}", "[]"],
                    "skip": [[";", "\n"], ["\"", "\""]]
                }
            },
            "handlers": {
                "check": ["Tools:Brackets:Check"]
            }
